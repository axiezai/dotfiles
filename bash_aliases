# Aliases

# cd behavior resolve typos
alias ..='cd ..'
alias cd..='cd ..'
alias dc='cd'

# enable color support: 
alias ls='ls -G'

# Copy personal zoom link:
alias zl='echo "https://weillcornell.zoom.us/my/axiezai" | pbcopy'

# ls aliases:
alias ll='ls -lahF'
alias la='ls -A'
alias l='ls -CF'

# mkdir on demand:
alias mkdir='mkdir -pv'

# most used:
alias gs='git status'
alias ga='git add -A'
alias jlab='jupyter lab'
alias jnb='jupyter notebook' 

#matlab
alias mnn='matlab -nosplash -nodesktop'
